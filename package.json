{
  "name": "vue3-vite-admin",
  "private": true,
  "version": "1.4.0",
  "scripts": {
    "dev": "vite --mode dev",
    "build": "vite build",
    "build:mock": "vite build --mode prod_mock",
    "preview": "vite preview",
    "preview:mock": "vite preview --mode prod_mock",
    "lint:eslint": "eslint --cache --max-warnings 0 \"{src,mock}/**/*.{vue,js,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"src/**/*.{vue,css,less,scss,sass}\"",
    "prepare": "husky install",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@amap/amap-jsapi-types": "^0.0.8",
    "@element-plus/icons-vue": "^2.0.1",
    "@types/nprogress": "^0.2.0",
    "@vueuse/core": "^8.6.0",
    "@wangeditor/editor": "^5.1.1",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "axios": "^0.27.2",
    "chroma-js": "^2.4.2",
    "driver.js": "^0.9.8",
    "echarts": "^5.3.3",
    "element-plus": "^2.2.2",
    "file-saver": "^2.0.5",
    "less": "^4.1.3",
    "lodash": "^4.17.21",
    "md-editor-v3": "^2.2.0",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.14",
    "sortablejs": "^1.15.0",
    "vue": "^3.2.37",
    "vue-router": "^4.0.15",
    "vue3-video-play": "1.3.1-beta.6",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@iconify/vue": "^3.2.1",
    "@types/chroma-js": "^2.1.3",
    "@types/file-saver": "^2.0.5",
    "@types/node": "^17.0.36",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-legacy": "^2.1.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.0.1",
    "husky": "^7.0.0",
    "lint-staged": "^12.4.2",
    "prettier": "^2.6.2",
    "standard-version": "^9.5.0",
    "stylelint": "^14.8.5",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.7.3",
    "unplugin-auto-import": "^0.8.6",
    "unplugin-icons": "^0.14.3",
    "unplugin-vue-components": "^0.19.6",
    "vite": "^3.1.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "^2.9.6",
    "vue-tsc": "^0.34.7"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ],
    "*.{css,scss,less,sass}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
